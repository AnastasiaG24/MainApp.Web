@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">
<head>

     <meta charset="utf-8">
     <title>AAG - Voyage</title>
     <link rel="icon" type="image/png" href="~/Content/img/airplane.jpg">
     <meta content="width=device-width, initial-scale=1.0" name="viewport">
     <meta content="" name="keywords">
     <meta content="" name="description">

     <!-- Google Web Fonts -->
     <link rel="preconnect" href="https://fonts.googleapis.com">
     <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
     <link href="https://fonts.googleapis.com/css2?family=Jost:wght@500;600&family=Roboto&display=swap" rel="stylesheet">

     <!-- Icon Font Stylesheet -->
     <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.15.4/css/all.css" />
     <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">
     <link rel="icon" type="image/png" href="Content/img/airplane.jpg">

     <!-- Libraries Stylesheet -->
     @Styles.Render("~/bundles/owlcarousel/css")
     @Styles.Render("~/bundles/lightbox/css")

     <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">

     <!-- Customized Bootstrap Stylesheet -->
     @Styles.Render("~/bundles/bootstrap/css")

     <!-- Template Stylesheet -->
     @Styles.Render("~/bundles/style/css")

     <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.4/jquery.min.js"></script>
     <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>

     <!-- JavaScript Libraries -->
     @Scripts.Render("~/bundles/easing/js")

     @Scripts.Render("~/bundles/waypoints/js")

     @Scripts.Render("~/bundles/owlscarousel/js")
     @Scripts.Render("~/bundles/lightbox/js")

     <!-- Template Javascript -->
     @Scripts.Render("~/bundles/main/js")

</head>

<body class="is-preload">

     <!-- Wrapper-->
     <div id="wrapper">
          <div id="main">
               <div class="inner">
                    <header id="header">
                         @Html.Partial("_Header")
                    </header>
               </div>
          </div>
     </div>
     @RenderBody()
     <footer id="footer">
          @Html.Partial("_Footer")
     </footer>

</body>
</html>